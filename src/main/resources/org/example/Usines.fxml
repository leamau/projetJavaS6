<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="705.0" prefWidth="889.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.UsinesController">
    <children>
        <Label contentDisplay="TOP" text="Visualisation Des Usines" textAlignment="CENTER">
         <font>
            <Font size="30.0" />
         </font></Label>
      <TableView prefHeight="482.0" prefWidth="850.0">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
        <Button fx:id="primaryButton" onAction="#switchToStocks" text="Revenir a la page des Stocks" />
      <Button fx:id="secondaryButton" onAction="#switchToAchats" text="Revenir a la page des Achats" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
